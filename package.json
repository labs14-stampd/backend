{
  "name": "backend",
  "version": "1.0.0",
  "description": "The back-end server for the Labs 14 Stampd project",
  "main": "index.js",
  "engines": {
    "node": "10.16.1",
    "yarn": "1.17.3"
  },
  "scripts": {
    "server": "nodemon index.js",
    "start": "node index.js",
    "test": "jest --watch --verbose",
    "lint": "npx eslint --fix '**/*.js'",
    "rollback": "npx knex migrate:rollback",
    "migrate": "npx knex migrate:latest",
    "seed": "npx knex seed:run",
    "redo": "yarn rollback && yarn migrate && yarn seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labs14-stampd/backend.git"
  },
  "keywords": [
    "Stampd",
    "GraphQL",
    "PostgreSQL",
    "Express",
    "Blockchain"
  ],
  "author": "Nathan Thomas, Aljoe Bacus, Brannan Conrad, Megan Jeffcoat, CJ Tantay, Byron Holmes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/labs14-stampd/backend/issues"
  },
  "homepage": "https://github.com/labs14-stampd/backend#readme",
  "dependencies": {
    "@sentry/node": "^5.5.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ethereumjs-tx": "1.3.7",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "graphql-playground-middleware-express": "^1.7.12",
    "helmet": "^3.19.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.19.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "web3": "1.0.0-beta.34"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  }
}
